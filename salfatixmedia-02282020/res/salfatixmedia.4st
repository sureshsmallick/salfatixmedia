  </Style>

  <Style name="Image.no_border" >
    <StyleAttribute name="border" value="none" />
  </Style>
  <Style name="Image.center">
     <StyleAttribute name="alignment" value="horizontalCenter" />
     <StyleAttribute name="border" value="none" />
  </Style>

  <Style name="Window.centerwindow" >
    <StyleAttribute name="position" value="center" />
    <StyleAttribute name='actionPanelPosition' value='none'/>
    <StyleAttribute name='ringMenuPosition' value='none'/>
    <StyleAttribute name='toolBarPosition' value='none'/>
    <StyleAttribute name="iosToolBarTintColor" value="#FFFFFF" />
  </Style>
  <Style name="Window.centerwindowpopup" >
    <StyleAttribute name='windowType' value='modal'/>
    <StyleAttribute name='sizable' value='no'/>
    <StyleAttribute name="position" value="center" />
    <StyleAttribute name='actionPanelPosition' value='none'/>
    <StyleAttribute name='ringMenuPosition' value='none'/>
    <StyleAttribute name='toolBarPosition' value='none'/>
  </Style>
  <Style name='Window.popup'>
    <StyleAttribute name='windowType' value='modal'/>
    <StyleAttribute name='sizable' value='no'/>
    <StyleAttribute name='position' value='center'/>
    <StyleAttribute name='actionPanelPosition' value='none'/>
    <StyleAttribute name='ringMenuPosition' value='none'/>
    <StyleAttribute name='toolBarPosition' value='top'/>
    <StyleAttribute name="statusBarType" value="none" />
  </Style>
  <Style name="Window.salfatixmenu" >
    <StyleAttribute name="position" value="center" />
    <StyleAttribute name='actionPanelPosition' value='none'/>
    <StyleAttribute name='ringMenuPosition' value='none'/>
    <StyleAttribute name='toolBarPosition' value='none'/>
  </Style>

  <Style name="Table.singleclick">
     <StyleAttribute name="rowActionTrigger" value="singleClick" />
  </Style>
  <Style name="Table.campaignselectedlist" >
    <StyleAttribute name="allowWebSelection" value="yes" />
