    <!--StyleAttribute name="fontWeight" value="bold" /-->
    <!-- set to 17 px like in the css -->
    <StyleAttribute name="fontSize" value="17px" />
  </Style>
  <Style name="Label.credential_header_mobile" >
    <StyleAttribute name="fontSize" value="large" />
    <StyleAttribute name="fontWeight" value="bold" />
  </Style>
  <Style name="Label.influencer_label_bold" >
    <StyleAttribute name="fontWeight" value="bold" />
  </Style>
  
  <Style name="Button.brand_html" >
    <StyleAttribute name="buttonType" value="link" />
    <StyleAttribute name="border" value="none" />
  </Style>
  <Style name="Button.brand_html_center" >
    <StyleAttribute name="buttonType" value="link" />
    <StyleAttribute name="alignment" value="center" />
    <StyleAttribute name="border" value="none" />
  </Style>
  <Style name="Button.credentials" >
    <StyleAttribute name="border" value="none" />
    <StyleAttribute name="textColor" value="#007AFF" />
  </Style>
  <Style name="Button.signin" >
    <StyleAttribute name="textColor" value="#007AFF" />
  </Style>
  <Style name="Button.highvisible">
     <StyleAttribute name="alignment" value="center" />
