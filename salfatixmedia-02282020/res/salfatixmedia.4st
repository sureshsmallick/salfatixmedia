  </Style>

  <!-- Style name="Group.no_border" >
    <StyleAttribute name="border" value="none" />
  </Style-->

  <Style name="ToolBarSeparator.myiosseparator" >
    <StyleAttribute name="iosSeparatorStretch" value="yes" />
  </Style>

  <Style name="Edit.influencer_brand_label_bold" >
    <StyleAttribute name="fontSize" value="large" />
    <StyleAttribute name="fontWeight" value="bold" />
  </Style>

  <Style name="Label.ff_formtitle" >
    <StyleAttribute name="fontSize" value="large" />
    <StyleAttribute name="fontWeight" value="bold" />
  </Style>
  <Style name="Label.brand_label_bold" >
    <StyleAttribute name="fontWeight" value="bold" />
  </Style>
