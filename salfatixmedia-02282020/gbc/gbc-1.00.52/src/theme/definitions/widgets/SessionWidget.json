{
  "name": "session",
  "title": "Session level specific variables",
  "description": "Configure variables specific to Session",
  "type": "group",
  "contents": [{
      "name": "gbc-SessionEndWidget-message-separator",
      "title": "Gbc SessionEndWidget message separator",
      "type": "color",
      "defaultValue": "$palette-text-secondary-12"
    },
    {
      "name": "gbc-SessionLogPromptWidget-color",
      "title": "Gbc SessionLogPromptWidget color",
      "type": "color",
      "defaultValue": "$theme-primary-color"
    },
    {
      "name": "gbc-SessionLogPromptWidget-background-color",
      "title": "Gbc SessionLogPromptWidget background color",
      "type": "color",
      "defaultValue": "$theme-secondary-color"
    },
    {
      "name": "gbc-SessionLogPromptWidget-z-index",
      "title": "Gbc SessionLogPromptWidget z index",
      "type": "number",
      "defaultValue": "$z-index-LogPrompt"
    },
    {
      "name": "gbc-SessionLogPromptWidget-button-height",
      "title": "Gbc SessionLogPromptWidget button height",
      "type": "size",
      "defaultValue": "$theme-field-default-height"
    },
    {
      "name": "gbc-WaitingWidget-color",
      "title": "Gbc WaitingWidget color",
      "type": "color",
      "defaultValue": "$theme-secondary-color"
    },
    {
      "name": "gbc-WaitingWidget-background-color",
      "title": "Gbc WaitingWidget background color",
      "type": "color",
      "defaultValue": "$theme-disabled-background-color"
    }
  ]
}
