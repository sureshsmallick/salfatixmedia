{
  "name": "ListViewWidget",
  "type": "group",
  "contents": [{
      "name": "gbc-ListViewWidget-margin-ratio",
      "type": "number",
      "defaultValue": "$theme-margin-ratio"
    },
    {
      "name": "gbc-ListViewWidget-table-margin",
      "type": "size",
      "defaultValue": "$theme-table-margin"
    },
    {
      "name": "gbc-ListViewWidget-background-color",
      "type": "color",
      "defaultValue": "$theme-field-background-color"
    },
    {
      "name": "gbc-ListViewWidget-border-width-top",
      "type": "size",
      "defaultValue": "$mt-field-border-width-top"
    },
    {
      "name": "gbc-ListViewWidget-border-width-right",
      "type": "size",
      "defaultValue": "$mt-field-border-width-right"
    },
    {
      "name": "gbc-ListViewWidget-border-width-bottom",
      "type": "size",
      "defaultValue": "$mt-field-border-width-bottom"
    },
    {
      "name": "gbc-ListViewWidget-border-width-left",
      "type": "size",
      "defaultValue": "$mt-field-border-width-left"
    },
    {
      "name": "gbc-ListViewWidget-border-color",
      "type": "color",
      "defaultValue": "$mt-field-border-color"
    },
    {
      "name": "gbc-ListViewWidget-focus-border-width-top",
      "type": "size",
      "defaultValue": "$mt-field-focus-border-width-top"
    },
    {
      "name": "gbc-ListViewWidget-focus-border-width-right",
      "type": "size",
      "defaultValue": "$mt-field-focus-border-width-right"
    },
    {
      "name": "gbc-ListViewWidget-focus-border-width-bottom",
      "type": "size",
      "defaultValue": "$mt-field-focus-border-width-bottom"
    },
    {
      "name": "gbc-ListViewWidget-focus-border-width-left",
      "type": "size",
      "defaultValue": "$mt-field-focus-border-width-left"
    },
    {
      "name": "gbc-ListViewWidget-focus-border-color",
      "type": "color",
      "defaultValue": "$mt-field-focus-border-color"
    },
    {
      "name": "gbc-ListViewWidget-currentRow-background-color",
      "title": "Gbc ListViewWidget currentRow background color",
      "type": "color",
      "defaultValue": "$theme-currentrow-background-color"
    },
    {
      "name": "gbc-ListViewWidget-currentRow-color",
      "title": "Gbc ListViewWidget currentRow text color",
      "type": "color",
      "defaultValue": "$theme-currentrow-color"
    },
    {
      "name": "gbc-ListViewWidget-min-width",
      "title": "Gbc ListViewWidget minimum width (pixels)",
      "type": "number",
      "defaultValue": 60
    },
    {
      "name": "gbc-ListViewWidget-min-page-size",
      "title": "Gbc ListViewWidget minimum page size",
      "type": "number",
      "defaultValue": 1
    }
  ]
}
