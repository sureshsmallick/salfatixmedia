{
  "name": "ToolbarWidget",
  "type": "group",
  "contents": [{
      "name": "gbc-ToolBarWidget-trigger-color",
      "type": "color",
      "defaultValue": "$theme-primary-background-color"
    },
    {
      "name": "gbc-ToolBarWidget-font-size-ratio",
      "type": "number",
      "defaultValue": "$theme-font-size-ratio"
    },
    {
      "name": "gbc-ToolBarWidget-hover-color",
      "type": "color",
      "defaultValue": "$theme-primary-faded-background-color"
    },
    {
      "name": "gbc-ToolBarSeparatorWidget-color",
      "type": "color",
      "defaultValue": "$theme-separator-color"
    },
    {
      "name": "gbc-ToolBarItemWidget-icon-size",
      "type": "size",
      "defaultValue": "$theme-small-icon-size"
    }
  ]
}
