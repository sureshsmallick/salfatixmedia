{
  "name": "AccordionFolderWidget",
  "type": "group",
  "contents": [{
      "name": "gbc-AccordionFolderWidget-title-margin",
      "title": "Gbc AccordionWidget title margin",
      "type": "number",
      "defaultValue": "$theme-margin-ratio*16px"
    },
    {
      "name": "gbc-AccordionFolderWidget-page-margin",
      "title": "Gbc AccordionWidget page margin",
      "type": "number",
      "defaultValue": "$theme-margin-ratio*10px"
    },
    {
      "name": "gbc-AccordionFolderWidget-font-size",
      "title": "Gbc AccordionWidget font size",
      "type": "number",
      "defaultValue": "$theme-font-size-ratio*18px"
    },
    {
      "name": "gbc-AccordionFolderWidget-color",
      "title": "Gbc AccordionWidget color",
      "type": "color",
      "defaultValue": "$theme-secondary-color"
    },
    {
      "name": "gbc-AccordionFolderWidget-background-color",
      "title": "Gbc AccordionWidget background color",
      "type": "color",
      "defaultValue": "$theme-header-color"
    }
  ]
}
