{
  "name": "mt-dialog",
  "type": "group",
  "contents": [{
      "name": "mt-dialog-margin-ratio",
      "type": "number",
      "defaultValue": "$theme-margin-ratio"
    },
    {
      "name": "mt-dialog-field-height-ratio",
      "type": "number",
      "defaultValue": "$theme-field-height-ratio"
    },
    {
      "name": "mt-dialog-z-index",
      "type": "number",
      "defaultValue": "$z-index-Dialog"
    },
    {
      "name": "mt-dialog-modal-z-index",
      "type": "number",
      "defaultValue": "$z-index-Modal"
    },
    {
      "name": "mt-dialog-background-color",
      "type": "color",
      "defaultValue": "$theme-secondary-alternative-background-color"
    },
    {
      "name": "mt-dialog-overlay-background-color",
      "type": "color",
      "defaultValue": "$theme-overlay-background-color"
    }
  ]
}
