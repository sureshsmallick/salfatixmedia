{
  "name": "mt-button",
  "type": "group",
  "contents": [{
      "name": "mt-button-margin-ratio",
      "type": "number",
      "defaultValue": "$theme-margin-ratio"
    },
    {
      "name": "mt-button-default-height",
      "type": "size",
      "defaultValue": "$theme-field-default-height"
    },
    {
      "name": "mt-button-default-width",
      "type": "size",
      "defaultValue": "$theme-button-default-width"
    },
    {
      "name": "mt-button-primary-background-color",
      "type": "color",
      "defaultValue": "$theme-primary-background-color"
    },
    {
      "name": "mt-button-primary-emphasis-background-color",
      "type": "color",
      "defaultValue": "$theme-primary-emphasis-background-color"
    },
    {
      "name": "mt-button-primary-faded-background-color",
      "type": "color",
      "defaultValue": "$theme-primary-faded-background-color"
    },
    {
      "name": "mt-button-primary-color",
      "type": "color",
      "defaultValue": "$theme-primary-color"
    },
    {
      "name": "mt-button-secondary-faded-color",
      "type": "color",
      "defaultValue": "$theme-secondary-faded-color"
    },
    {
      "name": "mt-button-disabled-color",
      "type": "color",
      "defaultValue": "$theme-disabled-color"
    },
    {
      "name": "mt-button-disabled-background-color",
      "type": "color",
      "defaultValue": "$theme-disabled-background-color"
    },
    {
      "name": "mt-button-separator-color",
      "type": "color",
      "defaultValue": "$theme-separator-color"
    },
    {
      "name": "mt-button-link-color",
      "type": "color",
      "defaultValue": "$html-anchor-color"
    },
    {
      "name": "mt-button-link-active-color",
      "type": "color",
      "defaultValue": "$html-anchor-active-color"
    }
  ]
}
