SCHEMA salfatixmedia

LAYOUT (style="centerwindow")
FOLDER
  PAGE(TEXT="Influencers",ACTION=refreshselected)
    GRID
    {
    [img_back1: :ff_formtitle2:                                                                                                                                                                                               :img_fak1]

    <G group2              > <T salfatixmedia2                                                                                                                                                                                         >
    [cmp_totalbudget_label ] [usr_instagr2|answer2|usr_usernam2|usr_firstnam2|usr_lastnam2  |usrmtc|usrmtc_f|cmpusrl_salfatixtota|cmpusrl_commen|cmpusrl_usergo|usr_coun|usr_stat|usr_cityname|usr|usr_addre|usr_addr|usr_add1|usr_add2]
    [cmp_totalbudget   |cur]                                                                
    [cmp_wantedusers_label ]
    [cmp_wantedusers       ]
    [cmp_selectioncomment  ]
    [                      ]
    [                      ]
    [lbl_estimated         ]
    [estimatedc       |cur1]
    [lbl_cost              ]
    [cost             |cur2]
    [lbl_already           ]
    [cmp_deposit      |cur3]
    [refreshselected       ]

    <                      >
    <G group4              >
    [validatedbybrand      ]
    [rejectedbybrand       ]
    [manageposts           ]
    <                      > <                                                                                                                                                                                                         >
    }
    END
  END
END --folder
END

TABLES
users1=users,
usermetrics1=usermetrics,
users,
usermetrics,
users2=users,
campaigns,
campaignuserlist,
countries,
states,
cities
END

ATTRIBUTES
Label lbl_estimated:campaigns_cmp_deposit_label3, text=%"Your estimated cost",hidden;
Label lbl_cost:campaigns_cmp_deposit_label6, text=%"Your cost",hidden;
Label lbl_already:campaigns_cmp_deposit_label1, text=%"You already paid",hidden;
Edit estimatedc=formonly.brandestimatedcost type DECIMAL(10,2), noEntry, scroll,hidden;
Edit cost=formonly.brandcost type DECIMAL(10,2), noEntry, scroll,hidden;
Label cur1                   = formonly.cur1,hidden;
Label cur2                   = formonly.cur2,hidden;
Label cur3                   = formonly.cur3,hidden;

LABEL ff_formtitle2=formonly.ff_formtitle2, justify=center, style="ff_formtitle";

Group group4:group4, text=%"Actions on influencer", style="no_border";
BUTTON validatedbybrand:validatedbybrand, text=%"Validate";
BUTTON rejectedbybrand:rejectedbybrand, text=%"Reject";
BUTTON manageposts:manageposts, text=%"Influencer posts";

Group group2:group2, text=%"Your campaign", style="no_border";
Label cmp_totalbudget_label : campaigns_cmp_totalbudget_label, text=%"Campaign budget";
Edit  cmp_totalbudget       = campaigns.cmp_totalbudget, noEntry;
Label cur                   = formonly.cur;
Label cmp_wantedusers_label : campaigns_cmp_wantedusers_label, text=%"Prefered number of influencers";
Edit  cmp_wantedusers       = campaigns.cmp_wantedusers, noEntry;
PHANTOM campaigns.cmp_publisheddate;
PHANTOM campaigns.cmp_selectionstartdate;
PHANTOM campaigns.cmp_selectionenddate;
TextEdit cmp_selectioncomment=campaigns.cmp_selectioncomment;
BUTTON refreshselected:refreshselected, text=%"Refresh list";
Image img_back1:img_back, action=backtocampaign, image="fa-arrow-circle-left", autoScale, sizePolicy=fixed, style="no_border", comment=%"Back to campaign";
Image img_fak1:img_fak, autoScale, sizePolicy=fixed, style="no_border";

